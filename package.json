{
  "name": "ng-svg-icon",
  "version": "1.0.1",
  "scripts": {
    "build": "gulp build",
    "build:watch": "gulp",
    "docs": "npm run docs:build",
    "docs:build": "compodoc -p tsconfig.json -n ng-svg-icon -d docs --hideGenerator",
    "docs:serve": "npm run docs:build -- -s",
    "docs:watch": "npm run docs:build -- -s -w",
    "lint": "tslint --type-check --project tsconfig.json src/**/*.ts",
    "lite": "lite-server",
    "playground:build": "tsc -p playground -w",
    "playground": "concurrently \"npm run build:watch\" \"npm run playground:build\" \"npm run lite\"",
    "test": "tsc && karma start",
    "publish-sample-app": "rm -rf sampleApp/ && cp -rf ./sample/ng-svg-icon-sample/www/ sampleApp/ && git add sampleApp && git commit -m 'Sample App Update' && git subtree push --prefix sampleApp origin gh-pages"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/guptasanchit90/ng-svg-icon"
  },
  "author": {
    "name": "Sanchit Gupta",
    "email": "gupta.sanchit90@gmail.com"
  },
  "keywords": [
    "angular"
  ],
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/guptasanchit90/ng-svg-icon/issues"
  },
  "devDependencies": {
    "@angular/common": "5.2.11",
    "@angular/compiler": "5.2.11",
    "@angular/compiler-cli": "5.2.11",
    "@angular/core": "5.2.11",
    "@angular/platform-browser": "5.2.11",
    "@angular/platform-browser-dynamic": "5.2.11",
    "@compodoc/compodoc": "1.0.5",
    "@types/jasmine": "2.5.53",
    "@types/node": "6.0.96",
    "angular-in-memory-web-api": "0.3.2",
    "codelyzer": "3.2.2",
    "concurrently": "3.5.1",
    "core-js": "2.5.3",
    "del": "2.2.2",
    "gulp": "3.9.1",
    "gulp-rename": "1.2.2",
    "gulp-rollup": "2.15.0",
    "jasmine-core": "2.6.4",
    "jasmine-spec-reporter": "4.1.1",
    "karma": "2.0.2",
    "karma-chrome-launcher": "2.1.1",
    "karma-cli": "1.0.1",
    "karma-coverage-istanbul-reporter": "1.4.3",
    "karma-jasmine": "1.1.1",
    "karma-jasmine-html-reporter": "0.2.2",
    "lite-server": "2.3.0",
    "node-sass": "4.9.0",
    "node-sass-tilde-importer": "1.0.1",
    "node-watch": "0.5.5",
    "protractor": "5.3.2",
    "rollup": "0.49.3",
    "run-sequence": "1.2.2",
    "rxjs": "5.5.6",
    "systemjs": "0.20.19",
    "ts-node": "3.2.2",
    "tslint": "5.7.0",
    "typescript": "2.4.2",
    "zone.js": "0.8.20"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}